<!DOCTYPE html>
<html>
<head>
  <title>WcsMentoringProject</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

    <div style="float:right">
      <% if current_user %>
        <%= link_to 'logout', logout_path %>
      <% else %>
         <%= link_to 'login', login_path %> |
	 <%= link_to 'register', register_path %>
      <% end %>
    </div>

<%= yield %>

</body>
</html>
